<Window x:Class="numsecurity.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:numsecurity"
        mc:Ignorable="d"
        Title="numerone's security" Height="150" Width="370">
    <StackPanel>
        <TextBlock x:Name="Intestazione"></TextBlock>
        <ComboBox x:Name="cbStato" SelectionChanged="cbStato_SelectionChanged">
            <ComboBoxItem x:Name="a">a</ComboBoxItem>
            <ComboBoxItem x:Name="b">b</ComboBoxItem>
            <ComboBoxItem x:Name="c">c</ComboBoxItem>
            <ComboBoxItem x:Name="d">d</ComboBoxItem>
        </ComboBox>
        <Button x:Name="btnok" Click="OnOk_Clicked"></Button>
        <TextBlock x:Name="txtRisultato" TextWrapping="Wrap" />
    </StackPanel>
</Window>
