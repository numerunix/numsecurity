<Window x:Class="numsecurity.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:numsecurity"
        mc:Ignorable="d"
        Title="numerone's security" Height="150" Width="350">
    <StackPanel>
        <TextBlock>Stato del driver USB su tutte le porte USB</TextBlock>
        <ComboBox x:Name="cbStato">
            <ComboBoxItem>Avvio Automatico (ritardato)</ComboBoxItem>
            <ComboBoxItem>Avvio Automatico</ComboBoxItem>
            <ComboBoxItem>Manuale</ComboBoxItem>
            <ComboBoxItem>Disabilato</ComboBoxItem>
        </ComboBox>
        <Button Click="OnOk_Clicked">Avvia</Button>
        <TextBlock x:Name="txtRisultato" TextWrapping="Wrap">Giulio Sorrentino © 2023, alcuni diritti riservati. Questo software è sotto GNU GPL o, secondo la tua personale opinione, qualsiasi versiome successiva. https://github.com/numerunix/numsecurity</TextBlock>
    </StackPanel>
</Window>
