<Window x:Class="numsecurity.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:numsecurity"
        mc:Ignorable="d"
        Title="numerone's security" Height="150" Width="350">
    <StackPanel>
        <TextBlock>USB Storage Driver state on all USB Ports</TextBlock>
        <ComboBox x:Name="cbStato" SelectionChanged="cbStato_SelectionChanged">
            <ComboBoxItem>Auto Start(delayed)</ComboBoxItem>
            <ComboBoxItem>Auto Start</ComboBoxItem>
            <ComboBoxItem>Manual</ComboBoxItem>
            <ComboBoxItem>Disabled</ComboBoxItem>
        </ComboBox>
        <Button x:Name="btnok" Click="OnOk_Clicked">Run</Button>
        <TextBlock x:Name="txtRisultato" TextWrapping="Wrap" />
    </StackPanel>
</Window>
